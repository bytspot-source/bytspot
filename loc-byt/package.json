{
      "name": "bytspot-web",
      "version": "0.1.0",
      "private": true,
      "engines": { "node": ">=18 <23" },
      "dependencies": {
            "@radix-ui/react-accordion": "^1.2.3",
            "@radix-ui/react-alert-dialog": "^1.1.6",
            "@radix-ui/react-aspect-ratio": "^1.1.2",
            "@radix-ui/react-avatar": "^1.1.3",
            "@radix-ui/react-checkbox": "^1.1.4",
            "@radix-ui/react-collapsible": "^1.1.3",
            "@radix-ui/react-context-menu": "^2.2.6",
            "@radix-ui/react-dialog": "^1.1.6",
            "@radix-ui/react-dropdown-menu": "^2.1.6",
            "@radix-ui/react-hover-card": "^1.1.6",
            "@radix-ui/react-label": "^2.1.2",
            "@radix-ui/react-menubar": "^1.1.6",
            "@radix-ui/react-navigation-menu": "^1.2.5",
            "@radix-ui/react-popover": "^1.1.6",
            "@radix-ui/react-progress": "^1.1.2",
            "@radix-ui/react-radio-group": "^1.2.3",
            "@radix-ui/react-scroll-area": "^1.2.3",
            "@radix-ui/react-select": "^2.1.6",
            "@radix-ui/react-separator": "^1.1.2",
            "@radix-ui/react-slider": "^1.2.3",
            "@radix-ui/react-slot": "^1.1.2",
            "@radix-ui/react-switch": "^1.1.3",
            "@radix-ui/react-tabs": "^1.1.3",
            "@radix-ui/react-toggle": "^1.1.2",
            "@radix-ui/react-toggle-group": "^1.1.2",
            "@radix-ui/react-tooltip": "^1.1.8",
            "@tanstack/react-query": "^5.85.6",
            "class-variance-authority": "^0.7.1",
            "clsx": "*",
            "cmdk": "^1.1.1",
            "embla-carousel-react": "^8.6.0",
            "input-otp": "^1.4.2",
            "lucide-react": "^0.487.0",
            "motion": "*",
            "react": "^18.3.1",
            "react-day-picker": "^8.10.1",
            "react-dom": "^18.3.1",
            "react-hook-form": "^7.55.0",
            "react-resizable-panels": "^2.1.7",
            "recharts": "^2.15.2",
            "sonner": "^2.0.3",
            "tailwind-merge": "*",
            "vaul": "^1.1.2"
      },
      "devDependencies": {
            "@types/node": "^20.10.0",
            "@vitejs/plugin-react-swc": "^3.10.2",
            "@playwright/test": "^1.40.0",
            "husky": "^9.1.7",
            "lint-staged": "^16.1.6",
            "vite": "6.3.5"
      },
      "scripts": {
            "dev": "vite",
            "dev:web": "vite",
            "build": "vite build",
            "preview:serve": "vite preview --port 5173 --strictPort",
            "test:e2e:preview": "playwright test -c playwright.preview.config.js",
            "dev:auth": "cross-env PORT=8090 MIGRATIONS_DIR=./migrations JWT_SECRET=dev_secret_change_me DEV_ENABLE_ADMIN_PROMOTE=true DATABASE_URL=postgresql://postgres:postgres@localhost:5432/bytspot?sslmode=disable go run ./services/auth-service/cmd/auth-service",
            "dev:venue": "go run ./services/venue-service/cmd/venue-service",
            "dev:bff": "cross-env PORT=3001 AUTH_SERVICE_URL=http://localhost:8090 VENUE_SERVICE_URL=http://localhost:8092 JWT_SECRET=dev_secret_change_me node services/gateway-bff/server.js",
            "dev:stack": "concurrently -k -n WEB,AUTH,VENUE,BFF -c blue,green,magenta,yellow \"npm:dev:web\" \"npm:dev:auth\" \"npm:dev:venue\" \"npm:dev:bff\"",
            "smoke": "node scripts/smoke.js",
            "tokens:build": "node packages/design-tokens/build.js",
            "tokens:import": "node packages/design-tokens/import-figma.js packages/design-tokens/figma-export.json",
            "tokens:fetch": "node packages/design-tokens/fetch-figma.js",
            "tokens:sync": "npm run tokens:fetch && npm run tokens:import && npm run tokens:build",
            "test:e2e": "playwright test",
            "test:e2e:ui": "playwright test --ui",
            "test:e2e:headed": "playwright test --headed",
            "test:screenshots": "playwright test --project=chromium tests/landing-page.spec.js",
            "preview:static": "playwright test -c playwright.static.config.js tests/preview-static.spec.js --project=chromium --headed",
            "preview:static:ui": "playwright test -c playwright.static.config.js tests/preview-static.spec.js --ui",
            "show-report": "playwright show-report",
            "playwright:install": "playwright install",
            "prepare": "husky install"
      }
,
  "lint-staged": {
    "services/gateway-bff/**/*.{js}": "npm --prefix services/gateway-bff run lint:staged --"
  }

}
